{
    "editor.tabSize": 2,
    "files.eol": "\n",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "files.exclude": {
      ".git": true,
      "server/node_modules": true,
      "server/dist": true,
      "client/node_modules": true,
      "client/dist": true
    },
    "editor.detectIndentation": false,
    "files.trimTrailingWhitespace": true
  }